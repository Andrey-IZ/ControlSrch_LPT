<catel:UserControl
    x:Class="Modules.Forms.CaptureFapch.Views.FormCaptureFapchView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:catel="http://catel.codeplex.com"
    xmlns:regions="http://prismlibrary.com/">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20*" />
            <ColumnDefinition Width="18*" />
            <ColumnDefinition Width="18*" />
        </Grid.ColumnDefinitions>
        <ContentControl
            Grid.Row="0"
            Grid.Column="0"
            Margin="10,0,10,10"
            regions:RegionManager.RegionName="SynthesizersRegion" />

        <ContentControl
            Grid.Row="0"
            Grid.Column="1"
            Margin="0,0,11,10"
            regions:RegionManager.RegionName="RebuildFrequencyRegion" />

        <ContentControl
            Grid.Row="0"
            Grid.Column="2"
            Margin="0,0,10,10"
            regions:RegionManager.RegionName="CaptureFapchRegion" />
    </Grid>

</catel:UserControl>
